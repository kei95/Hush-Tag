{
  "manifest_version": 3,
  "name": "Hush Tag - インプレッション稼ぎのクソリプを隠しちゃおう",
  "version": "1.2.0",
  "description": "インプレッション稼ぎのクソリプ達に静かにしてもらいましょう",
  "icons": {
    "16": "images/HushTag-16.png",
    "32": "images/HushTag-32.png",
    "48": "images/HushTag-48.png",
    "128": "images/HushTag-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.content.ts-94215e42-6f729368.js"
      ],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "resources": [
        "assets/content.ts-94215e42.js"
      ],
      "use_dynamic_url": true
    }
  ]
}